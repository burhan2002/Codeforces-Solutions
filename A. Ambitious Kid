// https://codeforces.com/problemset/problem/1866/A

#include <bits/stdc++.h>
using namespace std;

#define fast_io() ios::sync_with_stdio(false); cin.tie(nullptr);

int main() {
    fast_io();

    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        int ans = INT_MAX;
        for (int i = 0; i < n; i++) {
            int x;
            cin >> x;
            ans = min(ans, abs(x));
        }
        cout << ans << "\n";
    }

    return 0;
}
