#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t; 
    cin >> t;
    while (t--) {
        int n; 
        cin >> n;
        int odd = 0;
        for (int i = 0; i < n; ++i) {
            long long x; 
            cin >> x;
            if (x % 2 != 0) ++odd;
        }
        cout << (odd % 2 == 0 ? "YES" : "NO") << "\n";
    }
    return 0;
}
