#include <bits/stdc++.h>
using namespace std;

#define endl "\n"
#define fast_io() ios::sync_with_stdio(false); cin.tie(nullptr);

int main() {
    fast_io();

    int n;
    cin>>n;
    string p;
    cin>>p;
    string q;
    cin>>q;

    int l = p.size();
    int ans = 0;
    for(int i=0;i<l;i++)
    {
        int a = p[i]-'0';
        int b = q[i]-'0';

        int c = abs(a-b);
        int d = 10-abs(a-b);
        if(c<d)
            ans+=c;
        else ans+=d;
    }
    cout<<ans<<endl;

    return 0;
}
