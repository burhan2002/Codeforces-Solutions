#include <bits/stdc++.h>
using namespace std;

#define endl "\n"
#define fast_io() ios::sync_with_stdio(false); cin.tie(nullptr);

int main() {
    fast_io();

    int t;
    cin>>t;
    while(t--)
    {
        int a,b;
        cin>>a>>b;
        string s1,s2;
        cin>>s1>>s2;
        int ops=0;
        int f=0;

        while(s1.size()<=1000)
        {
            if(s1.find(s2)!=string::npos)
            {
                cout<<ops<<endl;
                f=1;
                break;
            }
            s1 = s1+s1;
            ops++;
        }
        if(!f)
            cout<<-1<<endl;
    }

    return 0;
}
