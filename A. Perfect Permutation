#include <bits/stdc++.h>
using namespace std;

#define fast_io() ios::sync_with_stdio(false); cin.tie(nullptr);

int main() {
    fast_io();

    int n;
    cin >> n;
    if (n % 2 == 1) {
        cout << -1 << "\n";
    } else {
        for (int i = 1; i <= n; i += 2) {
            cout << i+1 << " " << i << " ";
        }
        cout << "\n";
    }
    return 0;
}
