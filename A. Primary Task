#include <bits/stdc++.h>
using namespace std;

#define endl "\n"
#define fast_io() ios::sync_with_stdio(false); cin.tie(nullptr);

int main() {
    fast_io();

    int t;
    cin>>t;
    while(t--)
    {
        string s;
        cin>>s;
        if(s.size()>2 && s.substr(0,2)=="10")
        {
            int rest = stoi(s.substr(2));
            if(s[2]!='0' && rest>=2)
                cout<< "YES\n";
            else cout<< "NO\n";
        }
        else cout<< "NO\n";
    }

    return 0;
}
