#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef vector<int> vi;
typedef pair<int, int> pii;
typedef set<int>si;
const double PI =acos(-1);

#define pb push_back
#define srt(v) sort(v.begin(),v.end())
#define rsrt(v) sort(v.rbegin(),v.rend())

#define endl "\n"
#define fast_io() ios::sync_with_stdio(false); cin.tie(nullptr);

int main() {
    fast_io();

    int t;
    cin>>t;
    while(t--)
    {
        int p=8;
        char arr[p][p];
        for(int i=0;i<p;i++)
        {
            for(int j=0;j<p;j++)
            {
                cin>>arr[i][j];
            }
        }

        int a=0;

        for(int i=0;i<p;i++)
        {
            int f=0;
            for(int j=0;j<p;j++)
            {
                if(arr[i][j]=='R')
                {
                    f=1;
                }
                else
                {
                    f=0;
                    break;
                }
            }
            if(f==1)
            {
                cout<<'R'<<endl;
                a=1;
                break;
            }
        }
        if(a==0)
            cout<<'B'<<endl;
    }

    return 0;
}
